  <profile>
    <subsystem xmlns="urn:jboss:domain:logging:7.0">
      <console-handler name="CONSOLE">
        <formatter>
          <named-formatter name="COLOR-PATTERN"/>
        </formatter>
      </console-handler>
      <formatter name="PATTERN">
        <pattern-formatter pattern="%d{yyyy-MM-dd HH:mm:ss,SSS} %-5p [%c] (%t) %s%e%n"/>
      </formatter>
      <formatter name="COLOR-PATTERN">
        <pattern-formatter
          color-map="severe:brightblack,fatal:red,error:brightred,warn:yellow,warning:brightyellow,info:brightwhite,debug:green,trace:brightcyan,config:cyan,fine:brightblack,finer:blue,finest:black"
          pattern="%K{level}%d{HH:mm:ss,SSS} %-5p [%c] (%t) %s%e%n"/>
      </formatter>
      <logger category="com.arjuna">
        <level name="WARN"/>
      </logger>
      <logger category="io.jaegertracing.Configuration">
        <level name="WARN"/>
      </logger>
      <logger category="org.jboss.as.config" use-parent-handlers="false">
        <level name="DEBUG"/>
      </logger>
      <logger category="sun.rmi">
        <level name="WARN"/>
      </logger>
      <logger category="com.infoshareacademy" use-parent-handlers="false">
        <handlers>
          <handler name="DARIA_FILE"/>
          <handler name="CONSOLE"/>
        </handlers>
      </logger>
      <periodic-rotating-file-handler autoflush="true" name="FILE">
        <append value="true"/>
        <file path="server.log" relative-to="jboss.server.log.dir"/>
        <formatter>
          <named-formatter name="PATTERN"/>
        </formatter>
        <suffix value=".yyyy-MM-dd"/>
      </periodic-rotating-file-handler>
      <periodic-rotating-file-handler autoflush="true" name="DARIA_FILE">
        <append value="true"/>
        <file path="tmp/webapp.log"/>
        <formatter>
          <named-formatter name="PATTERN"/>
        </formatter>
        <level name="INFO"/>
        <suffix value=".yyyy-MM-dd"/>
      </periodic-rotating-file-handler>
      <root-logger>
        <handlers>
          <handler name="FILE"/>
          <handler name="CONSOLE"/>
        </handlers>
        <level name="INFO"/>
      </root-logger>
